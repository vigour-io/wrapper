{
  "name": "vigour-native",
  "version": "0.0.3",
  "author": "Vigour.io <dev@vigour.io>",
  "repository": {
    "type": "git",
    "url": "https://github.com/vigour-io/vigour-native"
  },
  "main": "./lib",
  "bin": {
    "vNative": "bin/build.js"
  },
  "scripts": {
    "installtestapp": "./installtestapp.js",
    "postinstall": "npm run installtestapp",
    "buildtestapp": "./buildtestapp.js",
    "pretest": "npm run buildtestapp",
    "test": "gaston test",
    "test-bridge": "gaston test -r browser -s test/phantom/bridge/",
    "test-plugins": "gaston test -r common -s test/common/",
    "test-builder": "npm run buildtestapp && gaston test -r node -s test/node/builder/",
    "test-ios": "npm run buildtestapp && gaston test -r node -s test/node/ios/",
    "test-android": "npm run buildtestapp && gaston test -r node -s test/node/android/",
    "test-samsungtv": "npm run buildtestapp && gaston test -r node -s test/node/samsungtv/"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "description": "The native code to wrap vigour apps",
  "contributors": [
    "Shawn Inder <shawn@vigour.io>",
    "Alexander Van Der Werff <alex@vigour.io>",
    "Michiel Van Liempt <michiel@vigour.io>",
    "Ranan Carvalho <renan@vigour.io>"
  ],
  "bugs": {
    "url": "https://github.com/vigour-io/vigour-native/issues"
  },
  "homepage": "https://github.com/vigour-io/vigour-native",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "archiver": "^0.14.4",
    "body-parser": "1.12.2",
    "browserify": "^9",
    "chai": "2.1.x",
    "checksum": "^0.1.1",
    "cli-spinner": "0.2.0",
    "colors": "1.0.x",
    "commander": "^2.7.1",
    "concat-stream": "^1.4.8",
    "element": "git+ssh://git@github.com:vigour-io/element.git#develop",
    "express": "4.12.3",
    "globby": "^2.1.0",
    "lodash": "^3.10.1",
    "mocha": "2.2.x",
    "mocha-phantomjs": "*",
    "mustache": "~0.8.1",
    "ncp": "^2.0.0",
    "npmlog": "0.1.x",
    "pliant": "git+ssh://git@github.com:vigour-io/pliant.git#develop",
    "plist": "^1.1.0",
    "promise": "^6.1.0",
    "rimraf": "^2.3.2",
    "shutter": "git+ssh@github.com:vigour-io/shutter.git#develop",
    "source-map": "0.1.32",
    "through": "^2.3.4",
    "vigour-fs": "^3.0.0",
    "vjs": "git+ssh://git@github.com:vigour-io/vjs.git#develop",
    "xcode": "^0.8.2"
  },
  "keywords": [
    "gaston",
    "mocha",
    "and",
    "chai",
    "phantom-js",
    "ci"
  ],
  "license": "BSD-2-Clause",
  "standard": {
    "globals": [
      "describe",
      "it",
      "expect",
      "before",
      "after",
      "beforeEach",
      "afterEach",
      "sinon"
    ]
  }
}
